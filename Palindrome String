LINK :: https://practice.geeksforgeeks.org/problems/palindrome-string0817/1

CODE :: 

class Solution {
    int isPalindrome(String S) {
        int i =0,j= S.length() - 1;
        while(i<j){
            if(S.charAt(i)==S.charAt(j)){
                i++;
                j--;
            }
            else{
                return 0;
            }
        }
        return 1;
    }
};
